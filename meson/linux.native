[binaries]

c = 'gcc'

strip   = 'strip'
windres = 'windres'

[host_machine]

system = 'linux'
cpu_family = 'x86_64'
cpu = 'x86_64'
endian = 'little'

[built-in options]

c_std = 'c99'
optimization = 's'
warning_level = '3'